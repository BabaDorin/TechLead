@model IEnumerable<TechLead.Models.Submission>
@using TechLead.Models
@{
    ViewBag.Title = "Submissions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="Content">

    <div class="TitleBox">
        <h2>Submissions</h2>
    </div>

    <div class="Box">
        <table>
            <tr>
                <th>Submission ID</th>
                <th>User</th>
                <th>Time</th>
                <th>Exercise ID</th>
                <th>Points</th>
                <th></th>
            </tr>
            @foreach (Submission S in Model)
            {
                <tr>
                    <td>@S.SubmissionID</td>
                    <td>@S.User</td>
                    <td>@S.Time</td>
                    <td>@S.ExerciseId</td>
                    <td>@S.Points</td>
                    <td>@Html.ActionLink("See More", "SubmissionDetails", "Problem", new { id = @S.SubmissionID }, null)</td>

                </tr>
            }
        </table>

        <br />
        @if (Request.UrlReferrer != null)
        {
            <a class="bottomNav" onclick="history.go(-1); return false;" href="#">Back</a><br />
        }

    </div>
</div>

