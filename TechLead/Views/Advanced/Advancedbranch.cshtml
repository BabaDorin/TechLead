@using PagedList;
@using PagedList.Mvc;
@using TechLead.Models;
@model IPagedList<TechLead.Models.Exercise>

@{
    ViewBag.Title = "Advanced";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="Content">

    <h2>Advanced</h2>
    <br />
    <br />
    <ul>
        @foreach (Exercise E in Model)
        {
            <li>
                @Html.ActionLink(E.Name, "Details", "Problem", new { id = E.Id }, null)
            </li>
        }
    </ul>
    @Html.PagedListPager(Model, page => Url.Action("Advancedbranch", "Advanced", new { page }))
</div>



