@model IEnumerable<TechLead.Models.Exercise>
@using TechLead.Models
@{
    ViewBag.Title = "Intermediate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="Content">

    <h2>Intermediate</h2>
    <br />
    <br />
    <ul>
        @foreach (Exercise E in Model)
        {
            <li>
                @Html.ActionLink(E.Name, "Details", "Problem", new { id = E.Id }, null)
            </li>
        }
    </ul>

</div>



